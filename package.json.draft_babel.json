{
  "name": "videojs-fairplay",
  "version": "0.2.0",
  "description": "Video.js tech for supporting Fairplay streaming.",
  "main": "src/videojs-fairplay",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all build:*",
    "build:js": "npm-run-all build:js:babel build:js:browserify build:js:uglify",
    "build:js:babel": "babel src -d es5",
    "build:js:browserify": "browserify . -s videojs-fairplay -g browserify-shim -o dist/videojs-fairplay.js",
    "build:js:uglify": "uglifyjs dist/videojs-fairplay.js --support-ie8 --comments -m -c -o dist/videojs-fairplay.min.js",
    "clean": "npm-run-all -p clean:*",
    "clean:build": "rimraf dist es5",
    "postclean:build": "mkdirp dist es5",
    "start": "webpack-dev-server --config webpack/webpack-dev-server",
    "lint:js": "eslint karma.conf.js src/ webpack/"
  },
  "keywords": [
    "html5",
    "player",
    "video",
    "videojs",
    "videojs-plugin"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "webpack/",
    "dist/",
    "grunt/",
    "index.html",
    "src/",
    "Gruntfile.js",
    "package.json",
    ".babelrc",
    ".eslintrc.yml"
  ],
  "browserify-shim": {
    "video.js": "global:videojs"
  },
  "author": "Carey Hinoki <carey.hinoki@gmail.com> (http://www.careyhinoki.me/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es3": "^1.0.1",
    "babelify": "^7.3.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "browserify": "^11.0.0",
    "browserify-shim": "^3.0.0",
    "clean-webpack-plugin": "^0.1.10",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "load-grunt-tasks": "^3.5.0",
    "moment": "^2.14.1",
    "npm-run-all": "^1.2.0",
    "time-grunt": "^1.4.0",
    "video.js": "^5.10.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
